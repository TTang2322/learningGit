"""
# Definition for a Node.
class Node:
    def __init__(self, val=None, children=None):
        self.val = val
        self.children = children
"""

class Solution:
    def __init__(self):
        self.res = list()
    def preorder(self, root: 'Node') -> List[int]:
        if not root:
            return
        if root:
            self.res.append(root.val)
            for ch in root.children:
                self.preorder(ch)
                
        return self.res
        